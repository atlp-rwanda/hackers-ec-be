    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
	"typescript-eslint": "^7.7.0"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix",
			"prettier --ignore-unknown --write",
			"git add"
		]
	}
}
