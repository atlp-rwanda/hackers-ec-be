    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.5",
    "passport-stub": "^1.1.1",
	"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.29.1",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "3.2.5",
	"@types/swagger-jsdoc": "^6.0.4",
